<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/comm_activity_background" >

    <include
        android:id="@+id/titlebar_locations_none"
        layout="@layout/layout_title_bar" />

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/titlebar_locations_none" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:id="@+id/img_locations_none_image"
                android:layout_width="@dimen/activity_checkin_none_img_checkin_none_image_2_layout_width"
                android:layout_height="@dimen/activity_checkin_none_img_checkin_none_image_3_layout_height"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/activity_locations_none_img_locations_none_image_0_layout_marginTop"
                android:src="@drawable/img_locate_fail" />

            <TextView
                android:id="@+id/tv_locations_none_tip_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/img_locations_none_image"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="false"
                android:layout_marginTop="@dimen/activity_locations_none_tv_locations_none_tip_1_1_layout_marginTop"
                android:text="@string/activity_familylocations_none_tip"
                android:textColor="@color/setting_item_text_color"
                android:textSize="@dimen/font_size_18" />

            <LinearLayout
                android:id="@+id/layout_locations_none"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_locations_none_tip_1"
                android:layout_marginTop="@dimen/activity_locations_none_layout_locations_none_2_layout_marginTop"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tv_locations_none_tip_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/comm_btn_margin"
                        android:layout_marginRight="@dimen/comm_btn_margin"
                        android:ellipsize="end"
                        android:text="@string/activity_familylocations_none_desc_1"
                        android:textColor="@color/textview_check_nobody_text_color"
                        android:textSize="@dimen/font_size_setting_item_name" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_locations_none_RelativeLayout_3_layout_marginTop" >

                    <TextView
                        android:id="@+id/tv_locations_none_tip_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/comm_btn_margin"
                        android:layout_marginRight="@dimen/comm_btn_margin"
                        android:ellipsize="end"
                        android:text="@string/activity_familylocations_none_list_1"
                        android:textColor="@color/textview_check_nobody_text_color"
                        android:textSize="@dimen/font_size_setting_item_name" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_locations_none_RelativeLayout_4_layout_marginTop"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/tv_locations_none_tip_4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/comm_btn_margin"
                        android:ellipsize="end"
                        android:text="@string/activity_familylocations_none_list_2"
                        android:textColor="@color/textview_check_nobody_text_color"
                        android:textSize="@dimen/font_size_setting_item_name" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_locations_none_RelativeLayout_5_layout_marginTop"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/tv_locations_none_tip_5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/comm_btn_margin"
                        android:layout_marginRight="@dimen/comm_btn_margin"
                        android:ellipsize="end"
                        android:text="@string/activity_familylocations_none_list_3"
                        android:textColor="@color/textview_check_nobody_text_color"
                        android:textSize="@dimen/font_size_setting_item_name" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_locations_none_RelativeLayout_6_layout_marginTop"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/tv_locations_none_tip_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/comm_btn_margin"
                        android:layout_marginRight="@dimen/comm_btn_margin"
                        android:ellipsize="end"
                        android:text="@string/activity_familylocations_none_list_4"
                        android:textColor="@color/textview_check_nobody_text_color"
                        android:textSize="@dimen/font_size_setting_item_name" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_locations_none_RelativeLayout_7_layout_marginTop"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/tv_locations_none_tip_7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/comm_btn_margin"
                        android:layout_marginRight="@dimen/comm_btn_margin"
                        android:ellipsize="end"
                        android:text="@string/activity_familylocations_none_list_5"
                        android:textColor="@color/textview_check_nobody_text_color"
                        android:textSize="@dimen/font_size_setting_item_name" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_locations_none_RelativeLayout_8_layout_marginTop"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/tv_locations_none_tip_9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/comm_btn_margin"
                        android:layout_marginRight="@dimen/comm_btn_margin"
                        android:ellipsize="end"
                        android:text="@string/activity_familylocations_none_list_6"
                        android:textColor="@color/textview_check_nobody_text_color"
                        android:textSize="@dimen/font_size_setting_item_name" />
                </RelativeLayout>
            </LinearLayout>

            <include
                android:id="@+id/callchart_common"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/layout_locations_none"
                android:layout_marginBottom="@dimen/activity_checkin_none_btn_checkin_none_add_11_layout_marginBottom"
                android:layout_marginTop="@dimen/activity_locations_none_LinearLayout_9_layout_marginTop"
                layout="@layout/layout_callchat" />
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>